  "bugs": {
    "url": "https://github.com/ajf-/StackEdit-Electron/issues"
  },
  "homepage": "https://github.com/ajf-/StackEdit-Electron#readme",
  "devDependencies": {
    "electron-packager": "^15.0.0",
    "electron-prebuilt": "^0.36.0"
  },
  "dependencies": {
    "stackedit": "git+https://git@github.com/benweet/stackedit.git"
  }
}
